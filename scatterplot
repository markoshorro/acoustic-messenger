


    
    temp_const = zeros(1,length(const));
    max_norm = 0; 

    for j=1:length(const)
        if(norm(const(j))>max_norm)
    
        max_norm = norm(const(j));
        end
    end



    for i=1:length(A_complex)
    
     temp_const(i)=const(i)./max_norm;
    
    end

	const = temp_const;
    
    scatterplot(const)
